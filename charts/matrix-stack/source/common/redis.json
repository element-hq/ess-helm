{
  "type": "object",
  "required": ["host"],
  "properties": {
    "host": {
      "type": "string",
      "description": "Redis host address"
    },
    "port": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535,
      "default": 6379
    },
    "db": {
      "type": "integer",
      "minimum": 0,
      "maximum": 15,
      "default": 0
    },
    "password": {
      "$ref": "file://common/credential.json"
    },
    "tls": {
      "type": "boolean",
      "default": false
    }
  },
  "additionalProperties": false
}
